    "sanity-plugin-asset-source-unsplash": "^3.0.1",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^19.1",
